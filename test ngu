    Tabs.Main1:AddButton(
        {
            Title = "Rain Fruit",
            Description = "But Old Fruit",
            Callback = function()
                for i, v in pairs(game:GetObjects("rbxassetid://14759368201")[1]:GetChildren()) do
                    v.Parent = game.Workspace.Map
                    v:MoveTo(
                        game.Players.LocalPlayer.Character.PrimaryPart.Position +
                            Vector3.new(math.random(-50, 50), 100, math.random(-50, 50))
                    )
                    if v:FindFirstChild("Fruit") and v.Fruit:FindFirstChild("AnimationController") then
                        local anim = v.Fruit:FindFirstChild("Idle")
                        if anim then
                            v.Fruit.AnimationController:LoadAnimation(anim):Play()
                        end
                    end
                    if v:FindFirstChild("Handle") then
                        v.Handle.Touched:Connect(
                            function(otherPart)
                                if otherPart.Parent == game.Players.LocalPlayer.Character then
                                    v.Parent = game.Players.LocalPlayer.Backpack
                                    game.Players.LocalPlayer.Character.Humanoid:EquipTool(v)
                                end
                            end
                        )
                    end
                end
            end
        }
    )

